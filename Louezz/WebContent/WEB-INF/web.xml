<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>carrental</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>/WEB-INF/index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>

  <error-page>
    <!-- Missing login -->
    <error-code>400</error-code>
    <location>/WEB-INF/Error.jsp</location>
</error-page>  
  <error-page>
    <!-- Missing login -->
    <error-code>401</error-code>
    <location>/WEB-INF/Error.jsp</location>
</error-page>
<error-page>
    <!-- Forbidden directory listing -->
    <error-code>403</error-code>
    <location>/WEB-INF/Error.jsp</location>
</error-page>
<error-page>
    <!-- Missing resource -->
    <error-code>404</error-code>
    <location>/WEB-INF/Error.jsp</location>
</error-page>
<error-page>
    <!-- Uncaught exception -->
    <error-code>500</error-code>
    <location>/WEB-INF/Error.jsp</location>
</error-page>
<error-page>
    <!-- Unsupported servlet method -->
    <error-code>503</error-code>
    <location>/WEB-INF/Error.jsp</location>
</error-page>
  
  <servlet>
  	<servlet-name>Home</servlet-name>
  	<servlet-class>com.Servelet.Home</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Home</servlet-name>
  	<url-pattern>/home</url-pattern>
  </servlet-mapping>
  
  
    <servlet>
  	<servlet-name>Profile</servlet-name>
  	<servlet-class>com.Servelet.Profile</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Profile</servlet-name>
  	<url-pattern>/profile</url-pattern>
  </servlet-mapping>
      <servlet>
  	<servlet-name>Logout</servlet-name>
  	<servlet-class>com.Servelet.Logout</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Logout</servlet-name>
  	<url-pattern>/logout</url-pattern>
  </servlet-mapping>
  
    <servlet>
  	<servlet-name>Login</servlet-name>
  	<servlet-class>com.Servelet.Login</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>Signup</servlet-name>
  	<servlet-class>com.Servelet.Signup</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Signup</servlet-name>
  	<url-pattern>/signup</url-pattern>
  </servlet-mapping>
  
    <servlet>
  	<servlet-name>Voiture1</servlet-name>
  	<servlet-class>com.Servelet.Voiture1</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Voiture1</servlet-name>
  	<url-pattern>/voiture1</url-pattern>
  </servlet-mapping>
    <servlet>
  	<servlet-name>Demande1</servlet-name>
  	<servlet-class>com.Servelet.Demande1</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Demande1</servlet-name>
  	<url-pattern>/demande1</url-pattern>
  </servlet-mapping>


  <servlet>
  	<servlet-name>Offre</servlet-name>
  	<servlet-class>com.Servelet.Offre</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Offre</servlet-name>
  	<url-pattern>/offre</url-pattern>
  </servlet-mapping>
  
  
</web-app>